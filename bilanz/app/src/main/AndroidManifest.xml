<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.leand.bilanztracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activitys.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activitys.OverviewActivity"
            android:label="Overview"/>
        <activity android:name=".Activitys.SalaryActivity"
            android:label="Salary"/>
        <activity android:name=".Activitys.ExpensesActivity"
            android:label="Expenses"/>
        <activity android:name=".Activitys.EditDeductionActivity"
            android:label="Add/Edit Deduction"/>
        <activity android:name=".Activitys.EditExpenseActivity"
            android:label="Add/Edit Expense"/>
        <activity android:name=".Activitys.OtherIncomeActivity"
            android:label="Other Income"/>
        <activity android:name=".Activitys.EditOtherIncomeActivity"
            android:label="Add/Edit Income"></activity>
    </application>

</manifest>